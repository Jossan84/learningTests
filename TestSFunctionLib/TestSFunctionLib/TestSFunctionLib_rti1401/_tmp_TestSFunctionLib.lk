LISTMAP PUBLICS,NOLOCALS,CROSSREFS
PUBLIC DS_PPC_BOARD = 1401

;*************************************************************************
; Specify starting address
;*************************************************************************

EXTERN  _go_startup
START   _go_startup


PUBLIC STACK_SIZE = 0x10000
PUBLIC DS_HEAP_SIZE = 0x10000   ; size of the part of heap reserved for
                                ; dSPACE programs, do not change this


;*************************************************************************
; Select a more effient heap library (dlmalloc)
;*************************************************************************
PUBLIC  malloc = dlmalloc
PUBLIC  free = dlfree
PUBLIC  calloc = dlcalloc
PUBLIC  zalloc = dlcalloc
PUBLIC  realloc = dlrealloc

ALIGN = 4
;*************************************************************************
; local memory
;*************************************************************************

ORDER    start_sec = 0x0        ; exception table and startup code,
                                ; do not change this
ORDER   .text
ORDER   .init
ORDER   initdat
ORDER   .rodata
ORDER   .rodata1
ORDER   .data
ORDER   .bss
ORDER   ioports
ORDER   .PPC.EMB.procflags
ORDER   SYSHOST

;-------------------------------------------------------------------------
; section for calibrated parameter set
;-------------------------------------------------------------------------
ORDER   calibrated_parameter_section


RESADD  0x00F00000, 0x02FFFFFF    ;no memory, flash

;*************************************************************************
; global memory
;*************************************************************************

ORDER   config_sect  = 0x03000000
ORDER   SlvFwSection = 0x03210000

RESADD  0x03400000, 0xFFFFFFFF    ;register, IO range, no memory
LOAD TestSFunctionLib.o03
LOAD ProcessFunction.o03
LOAD TestSFunctionLib_data.o03
LOAD TestSFunctionLib_trc_ptr.o03
LOAD rti_sim_engine.o03
LOAD rti_init_c.o03
LOAD rt_sim.o03
LOAD rti_assert.o03
LOAD "C:\dSPACE\dSPACE RCPHIL 2016-B\DS1401\RTPython\dsrtt1401.lib"
LOAD "C:\dSPACE\dSPACE RCPHIL 2016-B\DS1401\RTKernel\rtk1401.lib"
LOAD "C:\dSPACE\dSPACE RCPHIL 2016-B\DS1401\RTLib\ds1401.lib"
LOAD Z:\Workspace\TestSFunctionLib\TestSFunctionLib\libs\dspace\lib\libmatrix.a
LOAD "C:\dSPACE\dSPACE RCPHIL 2016-B\Compiler\PPCTools\LIB\mppc_b.lib"
LOAD "C:\dSPACE\dSPACE RCPHIL 2016-B\Compiler\PPCTools\LIB\mppchb.lib"
 
